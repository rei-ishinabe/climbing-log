  <%= simple_form_for @gym do |f| %>
    <%= f.input :name, label: false, placeholder: 'ジム名' %>
    <%= f.input :name_katakana, label: false, placeholder: 'ジム名(カタカナ)' %>
    <%= f.input :name_alphabet, label: false, placeholder: 'ジム名(アルファベット)' %>
    <%= f.input :address, label: false, placeholder: '住所' %>
    <%= f.input :latitude, label: false, placeholder: '緯度' %>
    <%= f.input :longitude, label: false, placeholder: '経度' %>
    <%= f.submit '登録', class:'btn btn-primary btn-block' %>
  <% end %>
