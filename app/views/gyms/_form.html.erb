  <%= simple_form_for @gym do |f| %>
    <%= f.input :name, label: 'ジム名' %>
    <%= f.input :name_katakana, label: 'ジム名(カタカナ)' %>
    <%= f.input :name_alphabet, label: 'ジム名(アルファベット)' %>
    <%= f.input :address, label: '住所' %>
    <%= f.input :latitude, label: '緯度' %>
    <%= f.input :longitude, label: '経度' %>
    <%= f.submit '登録', class:'btn btn-primary' %>
  <% end %>
